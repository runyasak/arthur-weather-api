{
  // Use this file as a starting point for your project's .eslintrc.
  // Copy this file, and add rule overrides as needed.
  "extends": "airbnb-base",
  "rules": {
    "no-console": 0,
    "import/prefer-default-export": "off",
    "indent": 2,
    "quotes": [2, "single"],
    "linebreak-style": [2, "unix"],
    "semi": [2, "never"]
  }
}
